# API Configuration
VITE_API_URL=https://mentourme-v2.onrender.com
VITE_WS_URL=wss://mentourme-v2.onrender.com

# App Configuration
VITE_APP_NAME=MentourMe
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional Mentorship Platform

# WebRTC Configuration (fetched securely from backend)
# No credentials stored in frontend - all WebRTC config comes from /api/webrtc/config endpoint
# This ensures Metered credentials are never exposed to browsers

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Performance
VITE_API_TIMEOUT=15000
VITE_CHAT_API_TIMEOUT=30000

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true
