# Development Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
VITE_API_URL=http://localhost:5000
VITE_WS_URL=ws://localhost:5000

# App Configuration
VITE_APP_NAME=MentourMe
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional Mentorship Platform

# WebRTC Configuration (Development - using Google STUN)
VITE_STUN_SERVER=stun:stun.l.google.com:19302

# Feature Flags
VITE_ENABLE_PWA=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Performance
VITE_API_TIMEOUT=15000
VITE_CHAT_API_TIMEOUT=30000

# Security
VITE_ENABLE_CSP=false
VITE_ENABLE_HTTPS_ONLY=false
